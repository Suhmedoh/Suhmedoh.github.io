.container {

}
#tracer {
	background-color: #e28e7f;
	height: 2px;
	padding: 0;
	margin: 0;
	margin-top: -40px;
	width: 100%;
	animation: stretch ease 6s;
	animation-iteration-count: infinite;
	animation-direction: alternate;
	-webkit-animation: stretch ease 6s;
	-webkit-animation-iteration-count: infinite;
	-webkit-animation-direction: alternate;
	-moz-animation: stretch ease 6s;
	-moz-animation-iteration-count: infinite;
}

@keyframes stretch{
  0% {
  	background: #e28e7f;
    transform: scaleX(0);
  }
  5% {
  	background: #e28e7f;
    transform: scaleX(0);
  }
  50% {
  	background: #778a53;
    transform: scaleX(1);
  }
  98% {
  	background: #e28e7f;
    transform: scaleX(0);
  }
  100% {
  	background: #e28e7f;
    transform: scaleX(0);
  }
}



@-moz-keyframes stretch{
  0% {
  	background: #e28e7f;
    -moz-transform:  scaleX(0);
  }
  5% {
  	background: #e28e7f;
    -moz-transform: scaleX(0);
  }
  50% {
  	background: #778a53;
    -moz-transform:  scaleX(1);
  }
  98% {
  	background: #e28e7f;
    -moz-transform:  scaleX(0);
  }
  100% {
  	background: #e28e7f;
    -moz-transform:  scaleX(0);
  }
}


p.style1 {
	margin-left: auto;
	margin-right: auto;
  margin-bottom: -40px;
	font-family: 'Share Tech Mono', monospace;
	font-size: 36px;
 	color: #e28e7f;
	text-align: center;
  padding: none;
	animation: pulse1 ease 6s;
	animation-iteration-count: infinite;
	animation-direction: alternate;
	-webkit-animation: pulse1 ease 6s;
	-webkit-animation-iteration-count: infinite;
	-webkit-animation-direction: alternate;
	-moz-animation: pulse1 ease 6s;
