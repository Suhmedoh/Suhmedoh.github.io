div .overlay {
  position: absolute;
  width: 100%;
  height: 100%;
  margin-left: 30px;
  margin-top: 30px;
  margin-bottom: 30px;
  padding-top: 100px;
  opacity: 0;
  z-index: 100;
  color: #d3d3d3;
  text-align: center;
  background: rgba(0,0,0,0);
  -moz-transition: all .5s;
  -webkit-transition: all .5s;
  transition: all .5s;
}


div .overlay:hover{
  opacity: 1;
  background: rgba(0,0,0,.8);
  -moz-transition: all .5s;
  -webkit-transition: all .5s;
  transition: all .5s;
}

