#bg-title-bottom1 {
  height: 1px; 
  padding: 0;
  margin-top: -10px;
  padding-bottom: 1px;
  width: 30%;
  margin-left: -40px;
  padding-left: 0px;
  float: left;
  position: absolute;
  background: linear-gradient(to right, transparent, #e28e7f);
  animation: bg-ani-a linear 6s;
  animation-iteration-count: infinite;
  animation-delay: .5s;
  -webkit-animation: bg-ani-a linear 6s;
  -webkit-animation-iteration-count: infinite;
  -webkit-animation-delay: .5s;
  -moz-animation: bg-ani-a linear 6s;
  -moz-animation-iteration-count: infinite;
  -moz-animation-delay: .5s;
}

@-moz-keyframes bg-ani-a{
  0% {
    left: 0px;
    top: 0px;
    -moz-transform: scaleX(0);
  }
  10% {
    left: 15px;
    top: 0px;
    -moz-transform: scaleX(.30);
  }
  50% {
    left: 150px;
    top: 0px;
    -moz-transform:  scaleX(.30);
  }
  90% {
    left: 285px;
    top: 0px;
    -moz-transform:  scaleX(.30);
  }
  100% {
    left: 300px;
    top: 0px;
    -moz-transform:  scaleX(0);
  }
}

@keyframes bg-ani-a{
  0% {
    left: 0px;
    top: 0px;
    -moz-transform: scaleX(0);
  }
  10% {
    left: 15px;
    top: 0px;
    -moz-transform: scaleX(.30);
  }
  50% {
    left: 150px;
    top: 0px;
    -moz-transform:  scaleX(.30);
  }
  90% {
    left: 285px;
    top: 0px;
    -moz-transform:  scaleX(.30);
  }
  100% {
    left: 300px;
    top: 0px;
    -moz-transform:  scaleX(0);
  }
}

#bg-title-bottom2 {
  height: 1px; 
  padding: 0;
  padding-bottom: 1px;
  width: 100%;
  position: relative;
  animation: bg-ani-b ease 10s;
  animation-iteration-count: infinite;
  -webkit-animation: bg-ani-b ease 10s;
  -webkit-animation-iteration-count: infinite;
  -moz-animation: bg-ani-b ease 10s;
  -moz-animation-iteration-count: infinite;
}

@-moz-keyframes bg-ani-b{
  0% {
    left: 0px;
    bottom: 0px;
    background: #e28e7f;
    -moz-transform:  scaleX(0);
  }
  10% {
    left: 30px;
    bottom: 0px;
    background: #e28e7f;
    -moz-transform: scaleX(.15);
  }
  50% {
    left: 150px;
    bottom: 0px;
    background: #778a53;
    -moz-transform:  scaleX(.15);
  }
  90% {
    left: 270px;
    bottom: 0px;
    background: #e28e7f;
    -moz-transform:  scaleX(.15);
  }
  100% {
    left: 300px;
    bottom: 0px;
    background: #e28e7f;
    -moz-transform:  scaleX(0);
  }
}

@keyframes bg-ani-b{
  0% {
    background: #e28e7f;
    -moz-transform:  scaleX(0);
  }
  10% {
    background: #e28e7f;
    -moz-transform: scaleX(.15);
  }
  50% {
